{
  "extends": "airbnb/legacy",
  "env": {
    "node": true
  },
  "globals": {
      "api": true
  },
  "rules": {
    "no-console": 0,
    "func-names": 0,
    "no-multi-spaces": 0,
    "key-spacing": 0,
    "new-cap": 0,
    "no-use-before-define": 0,
    "vars-on-top": 0,
    "padded-blocks": 0,
    "no-param-reassign": 0
  }
}
